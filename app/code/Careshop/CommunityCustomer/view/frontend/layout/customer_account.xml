<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="community_custom_profile">
                <arguments>
                    <argument name="path" xsi:type="string">community/customer/profile</argument>
                    <argument name="label" xsi:type="string">My Comunity Profile</argument>
                    <argument name="sortOrder" xsi:type="number">150</argument>
                    <argument name="navigation" xsi:type="boolean">true</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="community_custom_setting">
                <arguments>
                    <argument name="path" xsi:type="string">community/customer/setting</argument>
                    <argument name="label" xsi:type="string">My Comunity Settings</argument>
                    <argument name="sortOrder" xsi:type="number">155</argument>
                    <argument name="navigation" xsi:type="boolean">true</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>